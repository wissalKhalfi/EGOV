{% extends 'MyAppUserBundle:wissal:baseWissal.html.twig' %}

{% block content %}
<section id="headline">
    <div class="container  text-center">
        <h3>Gestion des extraits de naissance</h3>
    </div>
</section>

<section class="section">
    <div class="container ">
        <div class="col-lg-12 __web-inspector-hide-shortcut__">
            <section class="panel">
                <section class="panel">
                    <header class="panel-heading">
                        <h2><strong>Liste d'extraits de naissance</strong></h2>
                    </header>

                    <div class="panel-body">
                        <form >
                            <table class="table table-striped" id="table-example">
                                <thead>
                                    <tr>

                                        <th class="text-center">Id demande</th>		
                                        <th  class="text-center">Nom</th>
                                        <th class="text-center">Prenom</th>
                                        <th class="text-center">Date de naissance</th>
                                        <th class="text-center">Lieu de naissance</th>
                                        <th class="text-center">Sexe</th>
                                        <td> Afficher       &nbsp;&nbsp;&nbsp;&nbsp; </td>

                                    </tr>
                                </thead>
                                <tbody align="center">
                                    {#% for e in demandes_EN %#}
                                    <tr>
                                        <td> {#{e.id_demande}#} </td>
                                        <td> {#{e.nom}#} </td>
                                        <td> {#{e.prenom}#} </td>
                                        <td> {#{e.DateNaiss|date('Y-m-d H:i:s')}#} </td>
                                        <td> {#Lieu naissance#} </td>
                                        <td> {#Sexe #} </td>
                                        <td><a href="{#{ path ("#", {Iddemande:e.id_demande})}#} # ">Afficher</a></td>
                                    </tr> 
                                    {#% endfor %#}         
                                </tbody>
                            </table>
                        </form>
                    </div>
                </section>

            </section>







        </div>		
    </div>
</section>
  {% endblock %}